[user]
	name = kzwkzwkzw
	email = kzwkzwkzw@users.noreply.github.com
[core]
	editor = vim
[color]
	ui = auto
	status = auto
	diff = auto
	branch = auto
	interactive = auto
	grep = auto
[alias]
	st = status
	d = diff
	dh = diff --histogram
	dt = difftool
	dtd = difftool --dir-diff
	mt = mergetool
	a = add
	au = add -u
	c = commit -v
	ca = commit --amend -v
	b = branch
	ba = branch -a
	co = checkout
	l = log
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	sh = show
	ss = stash
	ssp = stash pop
	ssl = stash list
	sss = stash save
	ssa = stash apply
	ssd = stash drop
	ssc = stash clear
	ssb = stash branch
	pi = cherry-pick
	pia = cherry-pick --abort
	pic = cherry-pick --continue
	ri = rebase -i
	ra = rebase --abort
	rc = rebase --continue
[diff]
	tool = vimdiff
	algorithm = histogram
[merge]
	tool = vimdiff

